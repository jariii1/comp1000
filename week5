#include <iostream>

int addTwoNumbers(int a, int b) {
    return a + b;
}

void swapNumbers(int c, int d) {
    int temp = c;
    c = d;
    d = temp;
    // Note: This swap won't affect the original variables outside this function
}

double distance_between_points(double x1, double y1, double x2, double y2) {
    double dx = x2 - x1;
    double dy = y2 - y1;
    return std::sqrt(dx * dx + dy * dy);
}




int main() {
    std::cout << "3 + 4 = " << addTwoNumbers(3, 4) << '\n';
    std::cout << "-1 + 5 = " << addTwoNumbers(-1, 5) << '\n';

    int x = 10;
    int y = 20;
    std::cout << x << " + " << y << " = " << addTwoNumbers(x, y) << '\n';

    std::cout << "Before swap: x = " << x << ", y = " << y << '\n';
    swapNumbers(x, y); 
    std::cout << "After swap attempt: x = " << x << ", y = " << y << '\n';

    double dist = distance_between_points(1.0, 2.0, 4.0, 6.0);
    std::cout << "Distance between (1,2) and (4,6) = " << dist << '\n';

    return 0;
}

